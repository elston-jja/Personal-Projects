# include <stdio.h>

int main() {

  int var = 12;
  int* po;

  printf("The value for var currently is: %d", var);
  printf("The adress for var currenly is: %d", &var);

  *po=var;

  printf("\nWhen pointer po is set to the adress of var, it equals %d", po);
  printf("The adress for pointer var is: %d", &var );

  return 0;
}
